{
  "name": "dining-decider-api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "db:migrate": "wrangler d1 execute dining-decider-db --remote --file=./migrations/0001_initial.sql",
    "db:migrate:local": "wrangler d1 execute dining-decider-db --local --file=./migrations/0001_initial.sql",
    "db:seed": "npx tsx scripts/seed-database.ts",
    "db:seed:local": "npx tsx scripts/seed-database.ts --local"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.0",
    "@cloudflare/workers-types": "^4.20241218.0",
    "@types/node": "^22.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0",
    "wrangler": "^4.0.0"
  }
}
